{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "firstnewsp"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"Azure_DB_SRC_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Azure_DB_SRC'"
		},
		"LS_Sukhendra_Storage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_Sukhendra_Storage'"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "spcoreserver.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Trainingdatabase02"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "SQLadmin"
		},
		"Azure_DB_SRC_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "spcoreserver.database.windows.net"
		},
		"Azure_DB_SRC_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Trainingdatabase01"
		},
		"Azure_DB_SRC_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "SQLadmin"
		},
		"LS_Sukhendra_Storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sukhendra.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "firstnewsp_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"Azure_DB_SRC_password": {
						"value": "[parameters('Azure_DB_SRC_password')]"
					},
					"LS_Sukhendra_Storage_accountKey": {
						"value": "[parameters('LS_Sukhendra_Storage_accountKey')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"Azure_DB_SRC_properties_typeProperties_server": {
						"value": "[parameters('Azure_DB_SRC_properties_typeProperties_server')]"
					},
					"Azure_DB_SRC_properties_typeProperties_database": {
						"value": "[parameters('Azure_DB_SRC_properties_typeProperties_database')]"
					},
					"Azure_DB_SRC_properties_typeProperties_userName": {
						"value": "[parameters('Azure_DB_SRC_properties_typeProperties_userName')]"
					},
					"LS_Sukhendra_Storage_properties_typeProperties_url": {
						"value": "[parameters('LS_Sukhendra_Storage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}